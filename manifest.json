{
    "manifest_version": 2,
    "name": "UT Transcriptor",
    "description": "Base Level Extension",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "css":["dis.css"],
            "js": [
                "app.js"
            ]
        }
    ],
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "tabs",
        "*://*/*"
    ],
    "background": {
        "scripts": [
          "background.js"
        ],
        "persistent": true
      }    
}